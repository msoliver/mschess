{
    "name": "MSOliver Chess.com Chrome Extension",
    "version": "1.0",
    "description": "Chess.com extension",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "tabs",
        "alarms"
    ],
    "background": {
        "scripts": [
            "mschess.js",
            "jquery/dist/jquery.min.js",
            "chessapi.js"
        ],
        "persistant": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "options_page": "options.html",
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}